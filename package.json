{
  "name": "optimised",
  "description": "A fortnightly email newsletter about web performance.",
  "version": "0.0.1",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean:build -p build:*",
    "build:11ty": "eleventy --quiet",
    "build:rollup": "rollup -c",
    "clean:build": "rimraf _site",
    "clean:stage": "rimraf _dev",
    "dev": "npm-run-all clean:stage -p dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:rollup": "rollup -c -w"
  },
  "devDependencies": {
